<Application
    x:Class="FluentChat.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FluentChat">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
            <local:EmptyToBoolConverter x:Key="EmptyToBoolConverter"/>
            <local:EmptyToBoolConverter x:Key="EmptyToBoolReverseConverter" IsReverse="True"/>
            <local:EqualConverter x:Key="EqualConverter"/>
            <local:EqualConverter x:Key="NotEqualConverter" IsReverse="True"/>
            <local:JsonObjectConverter x:Key="JsonObjectConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
